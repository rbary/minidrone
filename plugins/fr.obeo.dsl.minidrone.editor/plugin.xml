<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated minidrone -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="fr.obeo.dsl.minidrone.presentation.MiniDroneModelWizardID"
            name="%_UI_MiniDroneModelWizard_label"
            class="fr.obeo.dsl.minidrone.presentation.MiniDroneModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/MiniDroneModelFile.gif">
         <description>%_UI_MiniDroneModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated minidrone -->
      <editor
            id="fr.obeo.dsl.minidrone.presentation.MiniDroneEditorID"
            name="%_UI_MiniDroneEditor_label"
            icon="icons/full/obj16/MiniDroneModelFile.gif"
            extensions="minidrone"
            class="fr.obeo.dsl.minidrone.presentation.MiniDroneEditor"
            contributorClass="fr.obeo.dsl.minidrone.presentation.MiniDroneActionBarContributor">
      </editor>
   </extension>

</plugin>
