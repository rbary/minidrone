<?xml version="1.0" encoding="UTF-8"?>

<fileset-config file-format-version="1.2.0" simple-config="true" sync-formatter="false">
  <local-check-config name="Minidrone Designer Checks" location="/fr.obeo.minidrone-designer.settings/CheckstyleConfiguration.xml" type="project" description="This checkstyle uses the same configuration used in Sirius project">
    <additional-data name="protect-config-file" value="false"/>
  </local-check-config>
  <fileset name="all" enabled="true" check-config-name="Minidrone Designer Checks" local="true">
    <file-match-pattern match-pattern="." include-pattern="true"/>
  </fileset>
  <filter name="FilesFromPackage" enabled="true">
    <filter-data value="src-gen"/>
  </filter>
</fileset-config>
